<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_lista_profesor"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.cristian.appestudiante.vista.ListaProfesor"
    android:orientation="vertical">

    <include android:id="@+id/toolbarlistaprofesor" layout="@layout/toolbar_lista"
        android:visibility="visible"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/listaProfesor" />

</LinearLayout>
